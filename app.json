{
  "expo": {
    "name": "cityweather",
    "description": "This project is really great.\nThe app randomly loads the weather of 7 cities. User can refresh the list to get new updates.\nThe search tab allows user to search for weather of a specific city",
    "slug": "cityweather",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dtd88.cityweather"
    },
    "android": {
      "package": "com.dtd88.cityweather"
    }
  }
}